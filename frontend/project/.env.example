# Auth0 Configuration
VITE_AUTH0_DOMAIN=your-tenant.region.auth0.com
VITE_AUTH0_CLIENT_ID=your-client-id
VITE_AUTH0_AUDIENCE=https://clarity-api.com

# API Configuration
VITE_API_URL=http://localhost:8000
VITE_PRODUCTION_API_URL=https://your-backend-url.railway.app
VITE_API_BASE_URL=https://your-backend-url.railway.app

# Environment
VITE_ENV=production

# Sentry Configuration
VITE_SENTRY_DSN=your-sentry-dsn
VITE_SENTRY_ENVIRONMENT=development # or production

# Rate Limiting
VITE_MAX_REQUESTS_PER_MINUTE=60

# Feature Flags
VITE_ENABLE_DARK_MODE=false
VITE_ENABLE_PREMIUM_FEATURES=false 